def defines_string_longer(str1, str2: str) -> str:
    """ accepts two string and returns the longer one"""
    return str1 if len(str1) > len(str2) else str2


def consists_of_numbers(lst: list) -> bool:
    """ a function that accepts a list
     and returns true if the list consists of only numbers, and false otherwise"""
    return all(isinstance(i, (int, float)) for i in lst)


def prints_line_to_console() -> None:
    """function that outputs a stream to the console and returns nothing"""
    print("*' * 80")
